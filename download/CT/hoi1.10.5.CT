 push rax

 cmp dword ptr [iEnableMQR],0
 je _ExitPP2                       // Jump if feature is disabled

 mov rax,[rbx+28]
 cmp rax,[pPlayer1]
 jne _ExitPP2

 cmp r8d,ecx
 jns _ExitPP2

