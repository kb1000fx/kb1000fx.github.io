 cmp dword ptr [iEnablePNP],0
 je _ExitPP1                       // Jump if feature is disabled

 mov rbx,[rdi+28]
 cmp rbx,[pPlayer1]
 jne _ExitPP1

 mov ebx,[rdi+30]
 add ebx,ebx

 cmp [rdi+38],ebx
 jns _ExitPP1

 mov [rdi+38],ebx

_ExitPP1:
