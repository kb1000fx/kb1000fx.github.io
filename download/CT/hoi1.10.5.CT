 cmp [rdi+28],ecx
 jns _ExitMFP

 mov [rdi+28],ecx

 mov edx,ecx

_ExitMFP:
 cmp edx,[r8+000003E8]            // Original code (get val)
 jmp _BackMFP                      // Back to main code

